{
  "$id": "https://github.com/kee007ney/2020Fall-BIOC-6223/blob/master/Week01/gene_schema_ulianova.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Genes",
  "type": "object",
  "required":[
    "name",
    "length",
    "accessionNum"
  ],
  "gene_properties": {
    "name": {
      "type": "string",
      "description": "Gene name"
    },
    "length": {
      "type": "integer",
      "description": "Length in base pairs"
    },
    "accessionNum": {
      "type": "string",
      "description": "Accession number"
    }
  }
}

