{
	"$schema": "http://json_schema.org/draft-07/schema#",
	"%id": "http://github.com/cdillard49/2020Fall-BIOC-6223/blob/master/week01/Homework 1 Dillard/cdillard_gene.json",
	"type": "object",
	"properties": {
		"gene_data": {
			"type": "array",
			"items": {
				"$ref": "#/definitions/gene"
			}
		}
	},
	"definitions": {
		"gene": {
			"type": "object",
			"required": [
				"gene_name",
				"gene_length",
				"gene_accs"
			],
			"properties": {
				"gene_name": {
					"type": "string",
					"description": "Name of the gene."
				},
				"gene_length": {
					"type": "integer",
					"minimum": 0,
					"description": "Length of the gene in base pairs."
				},
				"gene_accs": {
					"type": "string",
					"description": "Gene accession number."
				}
			}
		}
	}
}
